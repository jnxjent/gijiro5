{
  "version": "2.0",
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "functionTimeout": "00:10:00",
  "extensions": {
    "queues": {
      "messageEncoding": "None",
      "maxDequeueCount": 100,
      "visibilityTimeout": "01:00:00"
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": false       
      }
    },
    "logLevel": {
      "default": "Information",
      "Host.Results": "Trace",     
      "Host.Aggregator": "Trace",  
      "Function": "Trace",         
      "WebJobs.Script": "Trace",   
      "Worker": "Trace"            
    }
  }
}
